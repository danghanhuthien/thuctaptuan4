Public Shared Function changeToWords(ByVal numb As String) As String
    Dim val As String = "", wholeNo As String = numb, points As String = "", andStr As String = "", pointStr As String = ""
    Dim endStr As String = " đồng"
    Try
        Dim decimalPlace As Integer = numb.IndexOf(".")
        If decimalPlace &gt; 0 Then
            wholeNo = numb.Substring(0, decimalPlace)
            points = numb.Substring(decimalPlace + 1)
            If Convert.ToInt32(points) &gt; 0 Then
                andStr = " phẩy"
                pointStr = translateCents(points)
            End If
        End If
        val = String.Format("{0}{1}{2}{3}", translateWholeNumber(wholeNo).Trim(), andStr, pointStr, endStr)
    Catch
        ' Xử lý lỗi ở đây nếu cần
    End Try
    Return val
End Function

Private Shared Function translateWholeNumber(ByVal number As String) As String
    Dim word As String = ""
    Try
        Dim beginsZero As Boolean = False
        Dim isDone As Boolean = False
        Dim dblAmt As Double = Convert.ToDouble(number)
        If dblAmt &gt; 0 Then
            beginsZero = number.StartsWith("0")
            Dim numDigits As Integer = number.Length
            Dim pos As Integer = 0
            Dim place As String = ""
            Select Case numDigits
                Case 1
                    word = ones(number)
                    isDone = True
                Case 2
                    word = tens(number)
                    isDone = True
                Case 3
                    pos = (numDigits Mod 3) + 1
                    place = " Trăm "
                Case 4, 5, 6
                    pos = (numDigits Mod 4) + 1
                    place = " Nghìn "
                Case 7, 8, 9
                    pos = (numDigits Mod 7) + 1
                    place = " Triệu "
                Case 10
                    pos = (numDigits Mod 10) + 1
                    place = " Tỷ "
                Case Else
                    isDone = True
            End Select
            If Not isDone Then
                word = translateWholeNumber(number.Substring(0, pos)) + place + translateWholeNumber(number.Substring(pos))
                ' Xử lý số "lẻ"
                If number.Length &gt; 2 AndAlso number.Substring(0, 1) = "1" AndAlso number.Substring(1, 1) = "0" Then
                    word = word.Replace(" Một Trăm ", " Trăm lẻ ")
                End If
                ' Xử lý số không
                word = word.Replace(" Không Trăm ", " Không ")
                If beginsZero Then
                    word = " và " &amp; word.Trim()
                End If
            End If
            If word.Trim().Equals(place.Trim()) Then
                word = ""
            End If
        End If
    Catch
        ' Xử lý lỗi ở đây nếu cần
    End Try
    Return word.Trim()
End Function

Private Shared Function tens(ByVal digit As String) As String
    Dim digt As Integer = Convert.ToInt32(digit)
    Dim name As String = Nothing
    Select Case digt
        Case 10
            name = "Mười"
        Case 11
            name = "Mười một"
        Case 12
            name = "Mười hai"
        Case 13
            name = "Mười ba"
        Case 14
            name = "Mười bốn"
        Case 15
            name = "Mười lăm"
        Case 16
            name = "Mười sáu"
        Case 17
            name = "Mười bảy"
        Case 18
            name = "Mười tám"
        Case 19
            name = "Mười chín"
        Case 20
            name = "Hai mươi"
        Case 30
            name = "Ba mươi"
        Case 40
            name = "Bốn mươi"
        Case 50
            name = "Năm mươi"
        Case 60
            name = "Sáu mươi"
        Case 70
            name = "Bảy mươi"
        Case 80
            name = "Tám mươi"
        Case 90
            name = "Chín mươi"
        Case Else
            If digt &gt; 0 Then
                name = (tens(digit.Substring(0, 1) &amp; "0") &amp; " ") + ones(digit.Substring(1))
            End If
    End Select
    Return name
End Function

Private Shared Function ones(ByVal digit As String) As String
    Dim digt As Integer = Convert.ToInt32(digit)
    Dim name As String = ""
    Select Case digt
        Case 1
            name = "Một"
        Case 2
            name = "Hai"
        Case 3
            name = "Ba"
        Case 4
            name = "Bốn"
        Case 5
            name = "Năm"
        Case 6
            name = "Sáu"
        Case 7
            name = "Bảy"
        Case 8
            name = "Tám"
        Case 9
            name = "Chín"
    End Select
    Return name
End Function

Private Shared Function translateCents(ByVal cents As String) As String
    Dim cts As String = "", digit As String = "", vietOne As String = ""
    For i As Integer = 0 To cents.Length - 1
        digit = cents(i).ToString()
        If digit.Equals("0") Then
            vietOne = "Không"
        Else
            vietOne = ones(digit)
        End If
        cts += " " &amp; vietOne
    Next
    Return cts
End Function
